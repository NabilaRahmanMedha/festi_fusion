{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "src": "/api/v1/auth/(.*)", 
        "dest": "/index.js" 
      },
      { 
        "src": "/api/v1/events/(.*)", 
        "dest": "/index.js" 
      },
      { 
        "src": "/api/v1/users/(.*)", 
        "dest": "/index.js" 
      },
      { 
        "src": "/api/v1/review/(.*)", 
        "dest": "/index.js" 
      },
      { 
        "src": "/api/v1/booking/(.*)", 
        "dest": "/index.js" 
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  